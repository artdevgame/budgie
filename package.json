  "workspaces": [
    "graphql",
    "services",
    "web"
  ],
  "overrides": {
    "graphql": "16.5.0"
  },
  "devDependencies": {
    "@serverless-stack/cli": "1.14.4",
    "@serverless-stack/resources": "1.14.4",
    "@tsconfig/node16": "^1.0.3",
    "@types/node": "^18.7.18",
    "aws-cdk-lib": "2.39.1",
    "prettier": "^2.7.1",
    "typescript": "^4.8.3",
    "vitest": "^0.23.2"
  },
  "dependencies": {
    "react-native-web": "^0.17.7"
  }
}
